default: &default
  adapter: postgresql
  encoding: unicode
  port: 5432
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>
  timeout: 5000

development:
  <<: *default
  database: pocketmoney_development

test:
  <<: *default
  database: pocketmoney_test

production:
  <<: *default
  database: pocketmoney_production